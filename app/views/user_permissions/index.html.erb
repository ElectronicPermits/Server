<h1>Listing user_permissions</h1>

<table>
  <thead>
    <tr>
      <th>Action</th>
      <th>Target</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_permissions.each do |user_permission| %>
      <tr>
        <td><%= user_permission.action %></td>
        <td><%= user_permission.target %></td>
        <td><%= link_to 'Show', user_permission %></td>
        <td><%= link_to 'Edit', edit_user_permission_path(user_permission) %></td>
        <td><%= link_to 'Destroy', user_permission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User permission', new_user_permission_path %>
