<h1>Listing vehicles</h1>

<table>
  <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Color</th>
      <th>Year</th>
      <th>Inspection date</th>
      <th>License plate</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.make %></td>
        <td><%= vehicle.model %></td>
        <td><%= vehicle.color %></td>
        <td><%= vehicle.year %></td>
        <td><%= vehicle.inspection_date %></td>
        <td><%= vehicle.license_plate %></td>
        <td><%= link_to 'Show', vehicle %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
